
<%if admin_signed_in?%>
<h1>Admins List</h1>
<div class="container">
<table class="table table-striped table-hover">
<thead class="thead-dark">
<tr>
<th>Id</th>
<th>Name</th>
<th>Phone</th>
<th>Email</th>

<th colspan="2"></th>
    </tr>
    </thead>
    <tbody>
    <% @admin.each do |admin| %>
    <tr>
    <td> <%= admin.id %></td>
       <td> <%= admin.name %></td>
       <td> <%= admin.phone %></td>
       <td> <%= admin.email %></td>
       <td><%= link_to "edit admin", edit_admin_path(:id=>admin.id),class:"btn btn-warning",style:"color:white;" %></td>
       <td><%=
       link_to "Delete", 
       admins_id_path(:id=>admin.id),class:"btn btn-danger",style:"color:white;"
     %></td>
       </tr>
        <% end %>
        </tbody>
        </table>
</div>
<%else%>
You need to sign in with Admin
<%end%>